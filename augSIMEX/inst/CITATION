citHeader("To cite the 'augSIMEX' package in publications use:")


bibentry(bibtype = "article",
         title = "R package for analysis of data with mixed measurement error and misclassification in covariates: augSIMEX",
         author = c(person(given = "Qihuang", family = "Zhang"),
                    person(given = "Grace Y", family = "Yi")),
         journal = "Journal of Statistical Computation and Simulation",
         year = 2019,
         pages = "1--23",
         publisher="Taylor & Francis",
		 textVersion =
         paste("Zhang Q, Yi G Y.",  
  	       "R package for analysis of data with mixed measurement error and misclassification in covariates: augSIMEX[J].",
               "Journal of Statistical Computation and Simulation, 2019: 1-23."))